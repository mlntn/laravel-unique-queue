{
    "name": "mlntn/laravel-unique-queue",
    "type": "library",
    "description": "Laravel queue connection that prevents identical jobs from being queued",
    "keywords": ["laravel", "queue", "unique"],
    "homepage": "http://github.com/mlntn/laravel-unique-queue",
    "license": "MIT",
    "authors": [
        {
            "name": "Jared Mellentine",
            "email": "jared@mellentine.com"
        }
    ],
    "minimum-stability": "stable",

    "require": {
        "php": "~7.1",
        "illuminate/queue": "~5.7||~6||~7||~8",
        "illuminate/redis": "~5.7||~6||~7||~8"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mlntn\\Providers\\QueueServiceProvider"
            ]
        }
    }
}
